{
  "name": "nuxt-static-gzip",
  "version": "0.1.2",
  "description": "A module for Nuxt.js",
  "author": {
    "name": "frenchrabbit"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxt.js",
    "compression",
    "gzip",
    "brotli"
  ],
  "main": "lib/module.js",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "test": "jest",
    "test:unit": "jest --testRegex \"/test/unit/(.+)\\.test\\.js$\"",
    "test:system": "jest --testRegex \"/test/system/(.+)\\.test\\.js$\"",
    "test:coverage": "yarn test --coverage",
    "format": "prettier './**/*.{js,ts,json,vue}' --write"
  },
  "devDependencies": {
    "jest": "^23.4.1",
    "nuxt": "^2.4.0",
    "prettier": "^2.0.4",
    "puppeteer": "^1.6.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "compression-webpack-plugin": "^3.1.0",
    "consola": "^2.3.2",
    "express-static-gzip": "^2.0.6"
  }
}
