{
  "name": "nuxt-precompress",
  "version": "0.3.7",
  "description": "Precompress files to gzip and brotli and serve them automatically",
  "author": {
    "name": "frenchrabbit"
  },
  "keywords": [
    "vue",
    "nuxt",
    "nuxt.js",
    "module",
    "build",
    "compress",
    "compression",
    "gzip",
    "brotli"
  ],
  "main": "lib/module.js",
  "license": "MIT",
  "repository": "https://github.com/frenchrabbit/nuxt-precompress",
  "scripts": {
    "dev": "nuxt test/fixture",
    "test": "jest",
    "test:unit": "jest --testRegex \"/test/unit/(.+)\\.test\\.js$\"",
    "test:system": "jest --testRegex \"/test/system/(.+)\\.test\\.js$\"",
    "test:coverage": "yarn test --coverage",
    "format": "prettier './**/*.{js,ts,json,vue}' --write"
  },
  "devDependencies": {
    "jest": "^25.3.0",
    "nuxt": "^2.12.2",
    "prettier": "^2.0.4",
    "puppeteer": "^2.1.1",
    "serve-static": "^1.14.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "compression-webpack-plugin": "^3.1.0",
    "consola": "^2.3.2"
  }
}
